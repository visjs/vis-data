{
  "name": "vis-data",
  "version": "0.0.0-no-version",
  "description": "Manage unstructured data using DataSet. Add, update, and remove data, and listen for changes in the data.",
  "homepage": "http://visjs.org/",
  "license": "(Apache-2.0 OR MIT)",
  "repository": {
    "type": "git",
    "url": "https://github.com/visjs/vis-data.git"
  },
  "bugs": {
    "url": "https://github.com/visjs/vis-data/issues"
  },
  "keywords": [
    "vis",
    "visualization",
    "web based",
    "browser based",
    "javascript",
    "chart",
    "linechart",
    "timeline",
    "graph",
    "network",
    "browser"
  ],
  "main": "dist/umd.js",
  "module": "dist/esm.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "LICENSE*"
  ],
  "scripts": {
    "build": "npm run build:types && npm run build:code && npm run build:docs",
    "build:code": "rollup -c rollup.config.js",
    "build:docs": "typedoc ./src",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf '{docs,dist}/*'",
    "contributors:update": "git-authors-cli",
    "fix": "npm run lint -- --fix",
    "lint": "eslint '{src,test}/**/*.{m,}{j,t}s{x,}'",
    "postinstall": "opencollective postinstall || exit 0",
    "prepublishOnly": "npm run build",
    "test": "BABEL_ENV=test mocha",
    "test-cov": "BABEL_ENV=test-cov nyc mocha",
    "type-check": "tsc --noemit",
    "version": "npm run contributors:update && git add package.json",
    "watch": "rollup --watch --config rollup.config.js",
    "watch-dev": "npm run watch-dev"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "vis-util": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.5.4",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/sinon": "^7.0.13",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-plugin-istanbul": "^5.1.4",
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "git-authors-cli": "^1.0.18",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "mocha": "^6.1.4",
    "moment": "^2.24.0",
    "nyc": "^14.1.1",
    "opencollective": "^1.0.3",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.16.7",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-banner": "^0.2.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.23.0",
    "sinon": "^7.3.2",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3"
  },
